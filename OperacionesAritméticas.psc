Algoritmo OperacionesAritméticas
	Escribir "Ingrese dos números"
	Leer num1, num2;
	suma <- num1 + num2;
	resta <- num1 - num2;
	multiplica <- num1 * num2;
	divide <- num1 / num2;
	Escribir num1, " + ", num2, " = ", suma;
	Escribir num1, " - ", num2, " = ", resta;
	Escribir num1, " x ", num2, " = ", multiplica;
	Escribir num1, " ÷ ", num2, " = ", divide;
FinAlgoritmo
